<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Cake Love</title>

    @*Modernizer*@
    @Scripts.Render("~/bundles/modernizr")
    <script src="~/Scripts/ie6/warning.js"></script>
    <script type="text/javascript">
        window.onload = function(){
            function hasClass(element, cls) {
                return (' ' + element.className + ' ').indexOf(' ' + cls + ' ') > -1;
            }
            var body = document.getElementsByTagName('html')[0];
            var browserHasReqFeatures = hasClass(body, 'canvas');
            if (!browserHasReqFeatures)
            {
                e("scripts/ie6/");
            }
        };
    </script>
    @*End Modernizer*@

    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/bigfont")

</head>
<body>



    @RenderBody()

    @Scripts.Render("~/bundles/ng")
    @Scripts.Render("~/bundles/ngRoute")
    @Scripts.Render("~/bundles/ngSanitize")
    @Scripts.Render("~/bundles/ngUiBootstrapTpls")
    @Scripts.Render("~/bundles/ngUiBootstrap")
    @Scripts.Render("~/bundles/ngFileUpload")

    @Scripts.Render("~/bundles/polyfills")

    <script src="~/Scripts/holder.js"></script>
    <script src="~/Scripts/bse/bse.js"></script>

    @Scripts.Render("~/bundles/cakelove")
    @RenderSection("scripts", required: false)

</body>
</html>
